
#creating a function that returns a shopping list
remind_me <- function(){
  shop_list <- c("veggies", "berries", "bread", "cheese", "hummus", "crackers")
  return(shop_list)
}

#testing it
remind_me()
#Output:
#[1] "veggies"  "berries"  "bread"    "cheese"   "hummus"   "crackers"

